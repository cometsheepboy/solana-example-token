{
  "name": "token",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "lint": "npm run pretty && eslint .",
    "lint:fix": "npm run lint -- --fix",
    "flow": "set -ex; flow stop; flow-typed install jest@24; flow",
    "flow:watch": "watch 'flow' . --wait=1 --ignoreDirectoryPattern=/doc/",
    "lint:watch": "watch 'npm run lint:fix' . --wait=1",
    "bpf-sdk:update": "node_modules/@solana/web3.js/bin/bpf-sdk-install.sh",
    "build:program": "./src/program/do.sh build",
    "clean:program": "./src/program/do.sh clean",
    "localnet:update": "solana-localnet update",
    "localnet:up": "set -x; solana-localnet down; set -e; solana-localnet up",
    "localnet:down": "solana-localnet down",
    "localnet:logs": "solana-localnet logs -f",
    "pretty": "prettier --write '{,src/**/}*.js'",
    "postinstall": "npm run bpf-sdk:update",
    "test": "npm run build:program; cross-env NODE_ENV=test jest --useStderr",
    "test:cover": "npm run build:program; cross-env NODE_ENV=test jest --coverage --useStderr",
    "test:live": "npm run build:program; cross-env NODE_ENV=test TEST_LIVE=1 jest --useStderr",
    "test:watch": "npm run build:program; cross-env NODE_ENV=test jest --watch --useStderr"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
